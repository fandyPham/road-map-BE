{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "npx prisma generate"
    }
  },
  "env": {
    "PRISMA_GENERATE": "npx prisma generate"
  }
}
